{
  "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "src/proto/*"
    ]
  },
  "rules": {
    "no-empty": false,
    "no-console": false,
    "max-classes-per-file": false,
    "no-duplicate-imports": false,
    "no-floating-promises": false
  }
}
